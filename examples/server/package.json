{
  "name": "example-server",
  "description": "Example CXP server",
  "dependencies": {
    "ws": "^5.2.1"
  },
  "devDependencies": {
    "@types/ws": "^5.1.2",
    "parcel-bundler": "^1.9.6",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.1-insiders"
  },
  "scripts": {
    "example-server-node": "CX_MODE=websocket CX_ADDR=localhost:3877 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node --project tsconfig.json example-server-node.ts",
    "example-server-webworker:build": "parcel build --no-minify --detailed-report --experimental-scope-hoisting example-server-webworker.ts",
    "example-server-webworker:serve": "parcel serve --no-hmr --no-source-maps example-server-webworker.ts"
  }
}
